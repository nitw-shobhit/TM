<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="tm-core">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<non-jta-data-source>java:/jdbc/tmDS</non-jta-data-source>
		<mapping-file>META-INF/orm.xml</mapping-file>
		<class>com.tm.core.entity_sql.TmBase</class>
		<class>com.tm.core.entity_sql.TmUserInfo</class>
		<class>com.tm.core.entity_sql.TmProject</class>
		<class>com.tm.core.entity_sql.TmUserProject</class>
		<class>com.tm.core.entity_sql.TmModule</class>
		<class>com.tm.core.entity_sql.TmIssue</class>
		<class>com.tm.core.entity_sql.TmIssueComment</class>
		<class>com.tm.core.entity_sql.TmIssueAttachment</class>
		<class>com.tm.core.entity_sql.TmIssueHistory</class>
		<class>com.tm.core.entity_sql.TmIssueSubscribe</class>
		<class>com.tm.core.entity_sql.TmNotification</class>
		<class>com.tm.core.entity_sql.TmNotificationVariable</class>
		<class>com.tm.core.entity_sql.TmRelease</class>
		<class>com.tm.core.entity_sql.TmCalendar</class>
		<properties/>
	</persistence-unit>
</persistence>
